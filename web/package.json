{
  "name": "@ivory/web-spa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "styleguide": "npx styleguidist server --config src/styleguide/styleguide.config.js --open",
    "build:styleguide": "rm -rf stylebuild && npx styleguidist build --config src/styleguide/styleguide.config.js",
    "publish:styleguide": "npm run build:styleguide && aws s3 sync ./stylebuild s3://components.starterkit.com/ --profile switchboard",
    "format": "prettier-eslint \"$PWD/src/**/*.{js,jsx}\" --write",
    "lint:js": "eslint 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint 'src/**/*.{css,js,jsx}'",
    "lint": "npm run lint:js && npm run lint:css",
    "reinstall": "rm -rf node_modules && npx npm-force-resolutions && npm i",
    "eject": "react-scripts eject"
  },
  "config-overrides-path": "src/config/config-overrides",
  "devDependencies": {
    "eslint-plugin-babel": "^5.3.0",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-app-rewired": "^2.1.5",
    "react-styleguidist": "^11.0.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "dependencies": {
    "@ivoryio/kogaio": "^0.7.91",
    "@reach/router": "^1.3.3",
    "apollo-client": "^2.6.8",
    "apollo-utilities": "^1.3.3",
    "aws-amplify": "^2.2.6",
    "aws-amplify-react": "^3.1.7",
    "aws-appsync": "^3.0.2",
    "formik": "^2.1.4",
    "graphql-tag": "^2.10.3",
    "moment": "^2.24.0",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-apollo": "^3.1.4",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.4.0",
    "styled-components": "^5.0.1"
  },
  "resolutions": {
    "apollo-client": "^2.6.8",
    "apollo-utilities": "^1.3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
