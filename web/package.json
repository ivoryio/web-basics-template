    "lint:css": "stylelint './src/**/*.{css,js,jsx}'",
    "lint": "npm run lint:js && npm run lint:css",
    "eject": "react-scripts eject",
    "reinstall": "rm -rf node_modules && npx npm-force-resolutions && npm i"
  },
  "config-overrides-path": "src/config/config-overrides",
  "devDependencies": {
    "eslint-plugin-babel": "^5.3.0",
    "react-app-rewired": "^2.1.3",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "dependencies": {
    "@ivoryio/kogaio": "^0.7.61",
    "@reach/router": "^1.2.1",
    "apollo-client": "^2.6.4",
    "apollo-utilities": "^1.3.2",
    "aws-amplify": "^1.1.40",
    "aws-amplify-react": "^2.4.4",
    "aws-appsync": "^2.0.0",
    "formik": "^1.5.8",
    "graphql-tag": "^2.10.1",
    "moment": "^2.24.0",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-apollo": "^3.1.2",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0",
    "styled-components": "^4.4.0"
  },
  "resolutions": {
    "apollo-client": "^2.6.4",
    "apollo-utilities": "^1.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
