{
  "name": "ivory-web-spa-ci-cd",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "cdk": "cdk",
    "test": "jest",
    "watch": "tsc -w",
    "build": "rm -rf ./build && tsc",
    "nuke": "cdk destroy --app 'node build/infrastructure/app.js dfs-web-spa-ci-cd'",
    "deploy": "npm run build && cdk deploy --app 'node build/infrastructure/app.js' dfs-web-spa-ci-cd --output 'build/cdk.out' --strict"
  },
  "devDependencies": {
    "aws-cdk": "^1.16.2",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "^1.16.2",
    "@aws-cdk/aws-codebuild": "^1.16.2",
    "@aws-cdk/aws-codecommit": "^1.16.2",
    "@aws-cdk/aws-dynamodb": "^1.16.2",
    "@aws-cdk/aws-events-targets": "^1.16.2",
    "@aws-cdk/aws-iam": "^1.16.2",
    "@aws-cdk/aws-lambda": "^1.16.2",
    "@aws-cdk/aws-route53": "^1.16.2",
    "@aws-cdk/aws-route53-targets": "^1.16.2",
    "@aws-cdk/aws-s3": "^1.16.2",
    "@aws-cdk/core": "^1.16.2",
    "@types/node": "^12.12.7",
    "short-uuid": "^3.1.1",
    "source-map-support": "^0.5.9"
  }
}
